<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.EmployeeActivity">

    <TextView
        android:id="@+id/tradeoff"
        android:layout_width="match_parent"
        android:layout_height="53dp"
        android:text="TradeOff"
        android:textAlignment="center"
        android:textColor="#0BD54E"
        android:textColorLink="#DD2C00"
        android:textSize="40sp"
        android:textStyle="bold|italic"
        tools:ignore="MissingConstraints,RtlCompat"
        tools:layout_editor_absoluteX="-16dp"
        tools:layout_editor_absoluteY="39dp" />

    <Button
        android:id="@+id/mashovPhys"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="mashovPhys"
        android:text="mashov PHYSIOTHERAPY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.346"
        android:visibility="gone"/>

    <Button
        android:id="@+id/mashovCommun"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="mashovCommun"
        android:text="mashov communication"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.465"
        android:visibility="gone"/>

    <Button
        android:id="@+id/mashovIsuk"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="mashovIsuk"
        android:text="mashov isuk"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.579"
        android:visibility="gone"/>

    <TextView
        android:id="@+id/tvEmployeeTitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="@string/all_done_terms_signed"
        android:textColor="@color/black"
        android:textSize="24sp"
        android:textStyle="bold"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="-27dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="620dp"
        android:onClick="back"
        android:text="return"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.544"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>